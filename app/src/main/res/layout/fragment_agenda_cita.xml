        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.102" />

    <TextView
        android:id="@+id/Eetiqueda1"
        android:layout_width="110dp"
        android:layout_height="37dp"
        android:textSize="18sp"
